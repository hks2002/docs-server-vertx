{
  "http": {
    "port": 8090,
    "compressionSupported": false,
    "compressionLevel": 6,
    "decompressionSupported": true
  },
  "handler": {
    "session": {
      "sessionTimeout": 900000
    },
    "docs": {
      "windows": {
        "docsRoot": "c:/docs"
      },
      "linux": {
        "docsRoot": "/mnt/docs"
      },
      "waterMark": {
        "enable": true,
        "showCompany": true,
        "showBP": true,
        "showUser": true,
        "companyName": "DEDIENNE",
        "fileTypes": [
          "pdf",
          "jpg",
          "jpeg",
          "png",
          "bmp",
          "gif"
        ],
        "excludeNames": [
          "TDS",
          "OMSD",
          "GIM",
          "NOTICE",
          "GUIDE",
          "MANUAL"
        ]
      },
      "saveWithDocName": false
    },
    "upload": {
      "bodyLimit": -1,
      "folderDeep": 2,
      "folderLen": 3
    },
    "adServer": {
      "url": "ldap://127.0.0.1:389",
      "domain": "docs.com",
      "searchBase": "OU=User,DC=docs,DC=com"
    }
  },
  "mysql": {
    "jdbcUrl": "jdbc:mysql://localhost/docs",
    "user": "docs",
    "password": "password",
    "poolOptions": {
      "maximumPoolSize": 4,
      "minimumIdle": 1,
      "idleTimeout": 18000
    }
  },
  "mssql": {
    "jdbcUrl": "jdbc:sqlserver://127.0.0.1\\sage;DatabaseName=sage;TrustServerCertificate=true;",
    "user": "docs",
    "password": "password",
    "poolOptions": {
      "maximumPoolSize": 4,
      "minimumIdle": 0,
      "idleTimeout": 18000
    }
  }
}