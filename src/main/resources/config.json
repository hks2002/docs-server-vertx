{
  "http": {
    "port": 8090,
    "compressionSupported": false,
    "compressionLevel": 6,
    "decompressionSupported": true
  },
  "session": {
    "sessionTimeout": 3600000
  },
  "docs": {
    "docsRoot":{
      "windows": "Y:/Drawing",
      "linux":"/mnt/docs"
    },
    "waterMark": {
      "enable": true,
      "showCompany": true,
      "showBP": true,
      "showUser": true,
      "companyName": "DEDIENNE",
      "fileTypes": [
        "pdf",
        "jpg",
        "jpeg",
        "png",
        "bmp",
        "gif"
      ],
      "excludeNames": [
        "TDS",
        "OMSD",
        "GIM",
        "NOTICE",
        "GUIDE",
        "MANUAL"
      ]
    },
    "saveWithDocName": false,
    "expiryDays": 30
  },
  "upload": {
    "bodyLimit": -1,
    "folderDeep": 2,
    "folderLen": 3
  },
  "defaultAccess": {
    "read": true,
    "write": true,
    "admin": false
  },
  "adminPassword": "docs",
  "dmsServer": "http://127.0.0.1:4040",
  "dmsServerFast": "http://127.0.0.1:4040",
  "adServer": {
    "url": "ldap://127.0.0.1:389",
    "domain": "docs.com",
    "searchBase": "OU=User,DC=docs,DC=com"
  },
  "mysql": {
    "jdbcUrl": "jdbc:mysql://localhost/docs",
    "user": "docs",
    "password": "password",
    "poolOptions": {
      "maximumPoolSize": 4,
      "minimumIdle": 1,
      "idleTimeout": 18000
    }
  },
  "mssql": {
    "jdbcUrl": "jdbc:sqlserver://127.0.0.1\\sage;DatabaseName=sage;TrustServerCertificate=true;",
    "user": "docs",
    "password": "password",
    "poolOptions": {
      "maximumPoolSize": 4,
      "minimumIdle": 1,
      "idleTimeout": 18000
    }
  }
}
